<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <ClientWebPart Name="BrowserMessagingBroadcaster" Title="Browser Messaging Broadcaster" Description="Browser Messaging Broadcaster" DefaultWidth="200" DefaultHeight="100">

    <!-- Content element identifies the location of the page that will render inside the client web part
         Properties are referenced on the query string using the pattern _propertyName_
         Example: Src="~appWebUrl/Pages/ClientWebPart1.aspx?Property1=_property1_" -->
    <Content Type="html" Src="~appWebUrl/Pages/BrowserMessaging.aspx?EnableDebug=_EnableDebug_&amp;RebroadcastStrings=_RebroadcastStrings_" />

    <!-- Define properties in the Properties element.
         Remember to put Property Name on the Src attribute of the Content element above. -->
    <Properties>
      <Property Name="EnableDebug" Type="boolean" WebBrowsable="true" WebDisplayName="Enable Debug" WebDescription="Enable debugging. Received messages will be logged to screen" WebCategory="Properties" DefaultValue="false" RequiresDesignerPermission="true" />
      <Property Name="RebroadcastStrings" Type="boolean" WebBrowsable="true" WebDisplayName="Rebroadcast String Messages" WebDescription="Rebroadcast string messages to all iframes" WebCategory="Properties" DefaultValue="false" RequiresDesignerPermission="true" />
    </Properties>
   
